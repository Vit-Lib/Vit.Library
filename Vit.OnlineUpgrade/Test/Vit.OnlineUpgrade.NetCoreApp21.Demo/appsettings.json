{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",


  /* OnlineUpgrade配置(若不指定则不打开在线升级功能) */
  "OnlineUpgrade": {
    /* 在线升级api的地址前缀，如"/sys/onlineupgrade"(对应的在线升级首页为 /sys/onlineupgrade/index.html) */
    "apiPrefix": "/api/onlineupgrade",
    /* 升级密码，若不指定则不进行权限校验 */
    "password": "1",
    /* 首页显示的版本号，可不指定 */
    "appVersion": "1.0.0"
  }

}
